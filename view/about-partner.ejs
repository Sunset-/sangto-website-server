<!-- top -->
<%include about-top.ejs %>

    <div class="about-container partner-container">
        <div class="partner-content row">
            <% for(var i=0,item; item=partners[i++]; ) { %>
                <div class="partner-item col-xs-6 col-sm-4">
                    <div class="partner-img">
                        <img src="/upload/<%= item.cover %>" />
                    </div>
                    <p><%= item.title %></p>
                </div>
                <% } %>
        </div>
    </div>

    <!-- bottom -->
    <%include about-bottom.ejs %>