<!-- top -->
<%include about-top.ejs %>

    <div class="about-container honor-container">
        <div class="honor-content row">
            <% for(var i=0,item; item=honors[i++]; ) { %>
                <div class="honor-item col-xs-12 col-sm-4">
                    <div class="honor-img">
                        <img src="/upload/<%= item.cover %>" />
                    </div>
                    <p><%= item.title %></p>
                </div>
                <% } %>
        </div>
    </div>

    <!-- bottom -->
    <%include about-bottom.ejs %>